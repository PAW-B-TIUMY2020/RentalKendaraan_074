@model IEnumerable<RentalKendaraan_074.Models.Peminjaman>

@{
    ViewData["Title"] = "Index";
}
<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">

<h2>Tambah Data Peminjaman</h2>

<p>
    <a asp-action="Create">Tambah Data Baru</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.TglPeminjaman)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Biaya)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdCostumerNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdJaminanNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdKendaraanNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.TglPeminjaman)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Biaya)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdCostumerNavigation.IdCustomer)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdJaminanNavigation.IdJaminan)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdKendaraanNavigation.IdKendaraan)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdPeminjaman"><i class="fa fa-pencil" style="color:steelblue"></i></a> |
                <a asp-action="Details" asp-route-id="@item.IdPeminjaman"><i class="fa fa-server" style="color:steelblue"></i></a> |
                <a asp-action="Delete" asp-route-id="@item.IdPeminjaman"><i class="fa fa-trash" style="color:red"></i></a>
            </td>
        </tr>
}
    </tbody>
</table>
